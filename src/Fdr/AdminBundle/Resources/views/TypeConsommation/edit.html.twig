{% extends '::base.html.twig' %}
{% block title %}Type de Consommation|Edit{% endblock %}
{% block body -%}
<div class="page-header"><h1> Modification d'une Type de Consommation</h1></div>



{{ form_start(edit_form) }}
<div class="row">
<span class="alert-danger ">{{ form_errors(edit_form) }}</span>
</div>

<div class="row">

<div class="span6 offset3">

<fieldset>
<legend>Formulaire de modification</legend>

<div class="row-fluid">
<div class="span5">{{ form_label(edit_form.type," Type ") }}</div>
{{ form_widget(edit_form.type) }}<span class="alert-danger ">{{ form_errors(edit_form.type) }}</span>
</div>

<div class="row-fluid">
<div class="span5">{{ form_label(edit_form.agence," Agence ") }}</div>
{{ form_widget(edit_form.agence) }}<span class="alert-danger ">{{ form_errors(edit_form.agence) }}</span>
</div>

<div class="row-fluid">
<div class="span5">{{ form_label(edit_form.produit," Produit ") }}</div>
{{ form_widget(edit_form.produit) }}<span class="alert-danger ">{{ form_errors(edit_form.produit) }}</span>
</div>

<div class="row-fluid">
<div class="span5">{{ form_label(edit_form.prixUnitaire," Prix Unitaire ") }}</div>
{{ form_widget(edit_form.prixUnitaire) }}<span class="alert-danger ">{{ form_errors(edit_form.prixUnitaire) }}</span>
</div>


</fieldset>

</div>
</div>
{{ form_end(edit_form) }}
{% endblock %}

