{% extends '::base.html.twig' %}
{% block title %}Filiale|Edit{% endblock %}
{% block body -%}
<div class="page-header"><h1> Modification d'une Filiale</h1></div>



{{ form_start(edit_form) }}
<div class="row">
<span class="alert-danger ">{{ form_errors(edit_form) }}</span>
</div>

<div class="row">

<div class="span6 offset3">

<fieldset>
<legend>Formulaire de modification</legend>

<div class="row-fluid">
<div class="span5">{{ form_label(edit_form.libelle," Libelle ") }}</div>
{{ form_widget(edit_form.libelle) }}<span class="alert-danger ">{{ form_errors(edit_form.libelle) }}</span>
</div>

<div class="row-fluid">
<div class="span5">{{ form_label(edit_form.adresse," Adresse ") }}</div>
{{ form_widget(edit_form.adresse) }}<span class="alert-danger ">{{ form_errors(edit_form.adresse) }}</span>
</div>


</fieldset>

</div>
</div>
{{ form_end(edit_form) }}
{% endblock %}

