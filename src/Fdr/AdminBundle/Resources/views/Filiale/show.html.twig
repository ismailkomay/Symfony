{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Filiale</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ entity.code }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ entity.libelle }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ entity.adresse }}</td>
            </tr>
            <tr>
                <th>Champsup1</th>
                <td>{{ entity.champSup1 }}</td>
            </tr>
            <tr>
                <th>Champsup2</th>
                <td>{{ entity.champSup2 }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('filiale') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('filiale_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
