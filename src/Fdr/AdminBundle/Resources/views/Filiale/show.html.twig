{% extends '::base.html.twig' %}
{% block title %}Filiale|New{% endblock %}
{% block body -%}
<div class="page-header"><h1> Ajout d'une Filiale</h1></div>



{{ form_start(form) }}
<div class="desactiver-champs">
<div class="row">
<span class="alert-danger ">{{ form_errors(form) }}</span>
</div>

<div class="row">

<div class="span6 offset3">

<fieldset>
<legend>Formulaire d'ajout</legend>

<div class="row-fluid">
<div class="span5">{{ form_label(form.libelle," Libelle ") }}</div>
{{ form_widget(form.libelle) }}<span class="alert-danger ">{{ form_errors(form.libelle) }}</span>
</div>

<div class="row-fluid">
<div class="span5">{{ form_label(form.adresse," Adresse ") }}</div>
{{ form_widget(form.adresse) }}<span class="alert-danger ">{{ form_errors(form.adresse) }}</span>
</div>

</fieldset>

</div>
</div>
</div>
{{ form_end(form) }}
{% endblock %}

