{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Secteur</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ entity.code }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ entity.type }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ entity.libelle }}</td>
            </tr>
            <tr>
                <th>Champssupp1</th>
                <td>{{ entity.champssupp1 }}</td>
            </tr>
            <tr>
                <th>Champssupp2</th>
                <td>{{ entity.champssupp2 }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('secteur') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('secteur_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
