{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Secteur</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>
                    <ul>
                    {% for item in entity.typePrestations %}
                        <li> {{ item }}</li>
                    {% endfor %}
                    </ul>
                </td>
            </tr>
            <tr>
                <th>Clients</th>
                <td>
                    <ul>
                    {% for item in entity.clients %}
                        <li> {{ item }}</li>
                    {% endfor %}
                    </ul>
                </td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ entity.libelle }}</td>
            </tr>
            <tr>
                <th>Depart</th>
                <td>{{ entity.depart }}</td>
            </tr>
            <tr>
                <th>Arrivee</th>
                <td>{{ entity.arrivee }}</td>
            </tr>
            <tr>
                <th>Remarques</th>
                <td>{{ entity.remarques }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('secteur') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('secteur_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
