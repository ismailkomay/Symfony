{% extends '::base.html.twig' %}
{% block title %}TypePrestation|Show{% endblock %}
{% block body -%}
    <h1>TypePrestation</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ entity.libelle }}</td>
            </tr>
            <tr>
                <th>Secteurs</th>
                <td>
                    <ul>
                    {% for item in entity.secteurs %}
                        <li> {{ item }}</li>
                    {% endfor %}
                    </ul>
                </td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('typeprestation') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('typeprestation_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
