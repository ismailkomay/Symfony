{% extends '::base.html.twig' %}
{% block title %}Manutentionnaire|Edit{% endblock %}
{% block body -%}
<div class="page-header"><h1> Modification d'un manutentionnaire</h1></div>



{{ form_start(edit_form) }}
<div class="row">
<span class="alert-danger ">{{ form_errors(edit_form) }}</span>
</div>

<div class="row">

<div class="span6 offset3">

<fieldset>
<legend>Formulaire de modification</legend>

<div class="row-fluid">
<div class="span5">{{ form_label(edit_form.nom," Nom ") }}</div>
{{ form_widget(edit_form.nom) }}<span class="alert-danger ">{{ form_errors(edit_form.nom) }}</span>
</div>

<div class="row-fluid">
<div class="span5">{{ form_label(edit_form.prenom," Prenom ") }}</div>
{{ form_widget(edit_form.prenom) }}<span class="alert-danger ">{{ form_errors(edit_form.prenom) }}</span>
</div>

<div class="row-fluid">
<div class="span5">{{ form_label(edit_form.matricule," Matricule ") }}</div>
{{ form_widget(edit_form.matricule) }}<span class="alert-danger ">{{ form_errors(edit_form.matricule) }}</span>
</div>

<div class="row-fluid">
<div class="span5">{{ form_label(edit_form.type," Adresse ") }}</div>
{{ form_widget(edit_form.type) }}<span class="alert-danger ">{{ form_errors(edit_form.type) }}</span>
</div>

<div class="row-fluid">
<div class="span5">{{ form_label(edit_form.cin," CIN ") }}</div>
{{ form_widget(edit_form.cin) }}<span class="alert-danger ">{{ form_errors(edit_form.cin) }}</span>
</div>

<div class="row-fluid">
<div class="span5">{{ form_label(edit_form.tel," Telephone ") }}</div>
{{ form_widget(edit_form.tel) }}<span class="alert-danger ">{{ form_errors(edit_form.tel) }}</span>
</div>

<div class="row-fluid">
<div class="span5">{{ form_label(edit_form.adresse," Adresse ") }}</div>
{{ form_widget(edit_form.adresse) }}<span class="alert-danger ">{{ form_errors(edit_form.adresse) }}</span>
</div>



</fieldset>

</div>
</div>
{{ form_end(edit_form) }}
{% endblock %}

